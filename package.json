{
  "name": "maidrobe",
  "version": "0.1.0",
  "private": true,
  "description": "Digital closet management with AI-powered outfit suggestions",
  "workspaces": [
    "mobile",
    "edge-functions"
  ],
  "scripts": {
    "mobile:start": "pnpm --filter mobile start",
    "mobile:lint": "pnpm --filter mobile lint",
    "mobile:typecheck": "pnpm --filter mobile typecheck",
    "mobile:test": "pnpm --filter mobile test",
    "edge:dev": "pnpm --filter edge-functions dev",
    "edge:lint": "pnpm --filter edge-functions lint",
    "edge:typecheck": "pnpm --filter edge-functions typecheck",
    "edge:test": "pnpm --filter edge-functions test",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "test": "pnpm -r test"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/maidrobe.git"
  },
  "keywords": [
    "wardrobe",
    "closet",
    "outfit",
    "fashion",
    "ai",
    "react-native"
  ],
  "author": "",
  "license": "UNLICENSED"
}
